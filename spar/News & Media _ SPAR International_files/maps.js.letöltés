function localSparMapsInit(){var map=AmCharts.makeChart("localMap",{"type":"map","dataLoader":{"url":"/wp-content/themes/spar_international/mapdata/map.json","format":"json"},clickMapObject:function(m){if(m.url==null){location.href="/no-country/";}else{location.href=m.url;}},"areasSettings":{"autoZoom":false,"unlistedAreasColor":"#ff00ff","rollOverOutlineColor":"#7f7f7f","color":"#bdbdbd"},});}function keyfiguresSparMapsInit(){var map=AmCharts.makeChart("mapdiv",{"type":"map","dataLoader":{"url":"/wp-content/themes/spar_international/mapdata/map.json","format":"json"},clickMapObject:function(m){if(m.url==null){location.href="/no-country/";}else{location.href=m.url;}},"areasSettings":{"autoZoom":false,"unlistedAreasColor":"#ff00ff","rollOverOutlineColor":"#7f7f7f","color":"#bdbdbd"},"balloonLabelFunction":function(mapObject,ammap){console.log(mapObject);var html="<div class='tooltipData'><strong class='title'>"+mapObject.enTitle+"</strong><br/>";if(mapObject.joined!=null&&mapObject.joined.length>0){html+="Joined: <strong>"+mapObject.joined+"</strong><br/>";}if(mapObject.retailSales!=null&&mapObject.retailSales.length>0){html+="Retail sales '000: <strong>"+mapObject.retailSales+"</strong><br />";}if(mapObject.storenumber!=null&&mapObject.storenumber.length>0){html+="Retail stores: <strong> "+mapObject.storenumber+"</strong><br />";}if(mapObject.retailSalesm2!=null&&mapObject.retailSalesm2.length>0){html+="Retail sales m<sup>2</sup>: <strong>"+mapObject.retailSalesm2+"</strong>";}html+="</div>";return html;},});}